{
  "name": "basecamp-concierge-agent",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index-agent-sdk.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:examples": "echo 'skip examples build'",
    "clean": "rimraf node_modules && npm run clean:dbs",
    "clean:dbs": "rimraf *.db3* ||:",
    "dev": "npm run rebuild && npx --yes tsx --env-file .env --watch src/index-agent-sdk.ts",
    "dev:old": "npm run rebuild && npx --yes tsx --env-file .env --watch src/index.ts",
    "rebuild": "npm rebuild",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "gen:keys": "npx --yes tsx src/services/scripts/generateKeys.ts",
    "lint": "eslint .",
    "revoke": "tsx --env-file .env src/services/scripts/revokeInstallations.ts",
    "start": "node dist/index-agent-sdk.js",
    "schedule": "tsx --env-file .env src/services/scripts/admin-schedule.ts",
    "analytics": "node analytics.js",
    "analytics-cli": "node cli-analytics.js",
    "analytics-railway": "node railway-analytics.js",
    "analytics-direct": "node direct-api-analytics.js",
    "analytics-deployment": "node single-deployment-logs.js"
  },
  "dependencies": {
    "@bankr/sdk": "^0.1.0-alpha.5",
    "@coinbase/onchainkit": "^0.38.19",
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@neynar/nodejs-sdk": "^3.34.0",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@xmtp/agent-sdk": "^1.1.7",
    "@xmtp/content-type-primitives": "^2.0.2",
    "@xmtp/content-type-remote-attachment": "^2.0.2",
    "@xmtp/content-type-transaction-reference": "^2.0.2",
    "@xmtp/content-type-wallet-send-calls": "^2.0.0",
    "@xmtp/node-sdk": "^4.2.3",
    "axios": "^1.11.0",
    "better-sqlite3": "^11.7.0",
    "chrono-node": "^2.9.0",
    "dotenv": "^17.2.1",
    "ethers": "^6.15.0",
    "form-data": "^4.0.4",
    "hardhat": "^2.26.3",
    "langchain": "^0.3.31",
    "luxon": "^3.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uint8arrays": "^5.1.0",
    "viem": "^2.22.17",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.19.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/eslint__js": "^8.42.3",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.13.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.8",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "engines": {
    "node": ">=20 <24"
  }
}